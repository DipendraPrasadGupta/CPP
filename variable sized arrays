#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    
     int n,q,k,val,x,m,l;
     cin>>q;
     cin>>n;
     vector<int> v[n];
     for(int i=0; i<n; i++)
     {
         cin>>k;
         for(int j=0; j<k; j++)
         {
             cin>>val;
             v[i].push_back(val);
         }
     }
     for(x=0; x<q; x++)
     {
         cin>>m>>l;
         cout<<v[m][l];
         cout<<endl;
     }
     cout<<endl;
     return 0;
     
}
